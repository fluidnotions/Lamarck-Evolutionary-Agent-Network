# LLM Configuration
ANTHROPIC_API_KEY=your_key_here
MODEL_NAME=claude-3-haiku-20240307
BASE_TEMPERATURE=0.7

# Memory Configuration
EMBEDDING_MODEL=all-MiniLM-L6-v2
MEMORY_SCORE_THRESHOLD=7.0
MAX_MEMORIES_RETRIEVE=3

# Memory Decay Configuration (M3)
MEMORY_DECAY_LAMBDA=0.01

# Evolution Configuration
ENABLE_PARAMETER_EVOLUTION=true
EVOLUTION_LEARNING_RATE=0.1
MIN_TEMPERATURE=0.5
MAX_TEMPERATURE=1.0

# Trust Weighting Configuration (M2)
INITIAL_TRUST_WEIGHT=0.5
TRUST_LEARNING_RATE=0.1

# Visualization
ENABLE_VISUALIZATION=true
SHOW_MEMORY_RETRIEVAL=true
SHOW_PARAMETER_CHANGES=true

# LangGraph Configuration
STREAM_MODE=values
RECURSION_LIMIT=10

# Web Search Configuration (M1.6)
TAVILY_API_KEY=your_tavily_api_key_here
WEB_SEARCH_ENABLED=true
